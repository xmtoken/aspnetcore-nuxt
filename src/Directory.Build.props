<Project>
  <PropertyGroup>
    <Copyright>Copyright (c) 2020 xmtoken</Copyright>
    <NeutralLanguage>ja-JP</NeutralLanguage>
    <Product>AspNetCoreNuxt</Product>
    <Version>1.0.0</Version>
  </PropertyGroup>
  <PropertyGroup>
    <!-- if comment out, could not found source, on vscode debug -->
    <!-- <MapRoot>$([System.IO.Path]::GetFullPath('$(MSBuildThisFileDirectory)'))</MapRoot> -->
    <ProjectPrefixName>AspNetCoreNuxt</ProjectPrefixName>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyName>$(ProjectPrefixName).$(MSBuildProjectName)</AssemblyName>
    <CodeAnalysisRuleSet>..\Directory.Analyzers.ruleset</CodeAnalysisRuleSet>
    <Deterministic>true</Deterministic>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <LangVersion>8.0</LangVersion>
    <!-- if comment out, could not found source, on vscode debug -->
    <!-- <PathMap>$(MapRoot)=.</PathMap> -->
    <RootNamespace>$(ProjectPrefixName).$(MSBuildProjectName)</RootNamespace>
  </PropertyGroup>
</Project>
