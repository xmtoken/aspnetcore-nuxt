{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES5",
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/deep-equal",
      "@types/fontfaceobserver",
      "@types/lodash",
      "@types/node",
      "@types/nuxtjs__auth",
      "@types/ts-nameof"
    ]
  },
}
